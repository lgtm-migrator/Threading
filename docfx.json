{
  "metadata": [
    {
      "src": "bin/Release/netstandard2.0/Platform.$TRAVIS_REPO_NAME.dll",
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "*.md", "toc.yml" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "LinksPlatform's Platform.$TRAVIS_REPO_NAME Library",
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "dest": "_site",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
